{
  "name": "senml-js",
  "version": "1.0.0",
  "description": "SenML library written in JavaScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run lint && npm run typecheck && npm run test:all",
    "test:all": "mocha --require ts-node/register 'test/**/*.test.ts' --exit",
    "format": "prettier --write \"src/*.ts\"",
    "build": "tsc",
    "typecheck": "tsc --noEmit --pretty",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcotuna/senml-js.git"
  },
  "keywords": [
    "senml",
    "node",
    "javascript",
    "iot"
  ],
  "author": "Marco Santos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcotuna/senml-js/issues"
  },
  "homepage": "https://github.com/marcotuna/senml-js#readme",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-tsdoc": "^0.2.7",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
